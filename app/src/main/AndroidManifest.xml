<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cubipool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotransparent"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logotransparent"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OfferDetailActivity" />
        <activity android:name=".JoinCubiclesActivity" />
        <activity android:name=".ShareCubicleActivity" />
        <activity android:name=".ParticipantReservationPrototype" />
        <activity android:name=".ReservationDetailActivity" />
        <activity android:name=".ReservationsAvailablesActivity" />
        <activity android:name=".ReservationsAvailablesPrototype" />
        <activity android:name=".ReservationSucessActivity" />
        <activity android:name=".CubicleItemActivity" />
        <activity android:name=".CubicleAvailablesActivity" />
        <activity android:name=".SearchCubicleActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegisterSuccessActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity"/>

    </application>

</manifest>